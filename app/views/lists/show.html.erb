<div>
  <h2><%= @list.name %></h2>
  <ul>
    <% @list.bookmarks.each do |bookmark| %>
    <li>
      <p> Title: <%= bookmark.movie.title%> </p>
    <p> <%= bookmark.comment %> </p>
    </li>
    </ul>
  <% end %>
</div>
<%= link_to "Do you want to add a bookmark?", new_list_bookmark_path(@list), class: "btn btn-primary"%>
  <%= link_to "Back", lists_path, class: "btn btn-danger"%>
